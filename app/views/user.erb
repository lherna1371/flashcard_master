 <section class="user_page centerwrap">
  <% if current_user%>
  <%=current_user.email%>
  <%end%>
  <h1> Choose Deck </h1>
  <form action="/game" method="POST"> 
    <% @decks.each do |deck| %>
      <hr><input type="checkbox" name="params[<%= deck %>]" />
      <label for="params[<%= deck %>]"><%= deck.name %></label><br>
    <%end%>
    <hr>
    <input class="play" type="submit" value="Play" />
  </form>
 <section/>
